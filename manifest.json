{
    "manifest_version": 3,
    "name": "google spredsheet urls varify",
    "version": "1.0",
    "description": "Append tags to a Google Sheet",
    "permissions": [
      "storage",
      "identity"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "oauth2": {
      "client_id": "YOUR_CLIENT_ID.apps.googleusercontent.com",
      "scopes": [
        "https://www.googleapis.com/auth/spreadsheets"
      ]
    }
  }
  